@font-face {
  font-family: "Seesans";
  src: url({{ 'Seesans-1.004.woff' | file_url }}) format('woff'),
       url({{ 'Seesans-1.004.woff2' | file_url }}) format('woff2');
}

@font-face {
    font-family: 'Afacad';
